{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User Management Service for EV Co-ownership System",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "NODE_ENV=test jest --passWithNoTests",
    "test:watch": "NODE_ENV=test jest --watch --passWithNoTests",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run migrate:undo:all && npm run migrate && npm run seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ev-coownership/shared": "file:../shared",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^5.8.3",
    "sequelize": "^6.37.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3"
  },
  "keywords": [
    "microservice",
    "user-management",
    "ev-coownership"
  ]
}
