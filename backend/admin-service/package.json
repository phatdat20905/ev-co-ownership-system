{
  "name": "admin-service",
  "version": "1.0.0",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "admin",
    "microservice",
    "ev",
    "co-ownership"
  ],
  "author": "EV Co-ownership Team",
  "license": "ISC",
  "description": "Admin Service for EV Co-ownership System",
  "dependencies": {
    "@ev-coownership/shared": "file:../shared",
    "amqplib": "^0.10.9",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "mongodb": "^6.20.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3"
  }
}
